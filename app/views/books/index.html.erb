<!DOCTYPE html>
<html>
<head>
  <title>Bookers</title>
  <link rel="stylesheet" href="assets/stylesheets/books.sccs"/>
</head>

<h1>Books</h1>
<% @books.each do |book| %>
  <table>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
    <tr>
      <td>
        <%= book.title %>
        <%= book.body %>
      </td>
    </tr>
  </table>
  
<% end %>

</html>